  },
  "homepage": "https://github.com/PolymathNetwork/polymath-contract-artifacts/blob/master/README.md",
  "keywords": [
    "polymath",
    "artifacts",
    "json",
    "abi",
    "javascript",
    "typescript"
  ],
  "devDependencies": {
    "shx": "^0.2.2",
    "typescript": "3.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
