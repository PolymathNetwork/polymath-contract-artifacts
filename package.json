{
  "name": "@polymathnetwork/contract-artifacts",
  "version": "2.1.2-beta.3",
  "engines": {
    "node": ">=6.12"
  },
  "description": "Polymath smart contract compilation artifacts",
  "main": "lib/src/index.js",
  "license": "Apache-2.0",
  "files": [
